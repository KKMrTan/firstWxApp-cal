        <view class="item orange" bindtap="clickBtn" id="{{id1}}">退格</view>
        <view class="item orange" bindtap="clickBtn" id="{{id2}}">清空</view>
        <view class="item orange" bindtap="clickBtn" id="{{id3}}">+/-</view>
        <view class="item orange" bindtap="clickBtn" id="{{id4}}">+</view>
    </view>
    <view class="btn">
        <view class="item blue" bindtap="clickBtn" id="{{id5}}">7</view>
        <view class="item blue" bindtap="clickBtn" id="{{id6}}">8</view>
